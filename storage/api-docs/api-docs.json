{
    "openapi": "3.0.0",
    "info": {
        "title": "My API Docs",
        "version": "0.2"
    },
    "servers": [
        {
            "url": "/api"
        }
    ],
    "paths": {
        "/user/test": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "add new Rating",
                "operationId": "343e7a3fc5b09a5ebfb3a9a498d39d7d",
                "parameters": [
                    {
                        "name": "product_id",
                        "in": "path",
                        "description": "Product id",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "rating_value",
                                    "comment"
                                ],
                                "properties": {
                                    "rating_value": {
                                        "type": "number",
                                        "format": "float",
                                        "example": "4.7"
                                    },
                                    "comment": {
                                        "type": "string",
                                        "example": "vary good!"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Successful added",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "rating added seccessfully"
                                        },
                                        "rating": {
                                            "type": "string",
                                            "example": "[]"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request"
                    }
                },
                "security": [
                    {
                        "bearer": []
                    }
                ]
            }
        }
    },
    "tags": [
        {
            "name": "Users",
            "description": "Users"
        }
    ],
    "components": {
        "securitySchemes": {
            "bearer": {
                "type": "http",
                "scheme": "bearer",
                "in": "header",
                "name": "Authorization",
                "description": "Enter your Bearer token in the format `YOUR_TOKEN`. For example: `eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...`"
            }
        }
    }
}